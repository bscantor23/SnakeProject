package com.schemacatalogos.models;

import java.util.Optional;

public class Catalogo {

	private Producto producto;
	private Categoria categoria;
	
	private Catalogo() {
	}

	public Catalogo(Producto producto, Categoria categoria) {
		this.producto = producto;
		this.categoria = categoria;
	}
	
	public Catalogo(Optional<Producto> productoVer, Optional<Categoria> categoriaVer) {
		this.producto = productoVer.get();
		this.categoria = categoriaVer.get();
	}

	public Producto getProducto() {
		return producto;
	}

	public void setProducto(Producto producto) {
		this.producto = producto;
	}

	public Categoria getCategoria() {
		return categoria;
	}

	public void setCategoria(Categoria categoria) {
		this.categoria = categoria;
	}
	
}
